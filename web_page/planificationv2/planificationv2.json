    "exp_init" : {
      "type" : "expression",
      "value" : "if($data.configurations && $data.formInput.configurationsInput && !$data.formInput.configurationsInput.persistenceId){\n    $data.formInput.configurationsInput = $data.configurations\n}",
      "exposed" : false
    },
    "exp_tasks" : {
      "type" : "expression",
      "value" : "return {\n    convertToTasks:function(acts){\n        var tasks = [];\n        var exit = false;\n        acts.forEach(function(a){\n            if(a.dateFin && a.dateFin!==\"\" && a.dureeJours && a.dureeJours >0){\n                var start = angular.copy(a.dateFin);\n                start.setDate(a.dateFin.getDate()-a.dureeJours)\n                var end = angular.copy(a.dateFin);\n                tasks.push({\"name\":a.codeActivite +\" - \"+a.nomActivite,\"end\":end, \"start\":start})\n            }else{\n                exit = true;\n            }    \n        })\n        return exit?exit: tasks;\n    }\n}",
      "exposed" : false
    }
